{
    "name": "rector/website",
    "description": "Website of GetRector.org",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "doctrine/annotations": "^1.10",
        "doctrine/cache": "^1.10",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.7",
        "erusev/parsedown-extra": "^0.8.1",
        "ocramius/proxy-manager": "2.8.*",
        "psr/simple-cache": "^1.0",
        "ramsey/uuid": "^3.9",
        "ramsey/uuid-doctrine": "^1.6",
        "rector/rector": "dev-master",
        "sentry/sentry-symfony": "^3.4",
        "symfony/asset": "*",
        "symfony/cache": "*",
        "symfony/console": "*",
        "symfony/debug-bundle": "*",
        "symfony/dependency-injection": "*",
        "symfony/dotenv": "*",
        "symfony/event-dispatcher": "*",
        "symfony/flex": "^1.4",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/http-kernel": "*",
        "symfony/intl": "*",
        "symfony/mime": "*",
        "symfony/orm-pack": "^1.0.7",
        "symfony/process": "*",
        "symfony/security-bundle": "*",
        "symfony/stopwatch": "*",
        "symfony/templating": "*",
        "symfony/translation": "*",
        "symfony/twig-bridge": "*",
        "symfony/twig-bundle": "*",
        "symfony/validator": "*",
        "symfony/web-profiler-bundle": "*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "*",
        "symplify/autodiscovery": "^9.0",
        "symplify/autowire-array-parameter": "^9.0",
        "symplify/flex-loader": "^9.0",
        "symplify/package-builder": "^9.0",
        "symplify/smart-file-system": "^9.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.34",
        "phpstan/phpstan-doctrine": "^0.12",
        "phpunit/phpunit": "^9.0|^10.0",
        "roave/security-advisories": "dev-master",
        "slevomat/coding-standard": "^6.3",
        "symfony/browser-kit": "*",
        "symfony/css-selector": "*",
        "symfony/var-dumper": "*",
        "symplify/coding-standard": "^9.0",
        "symplify/easy-coding-standard": "^9.0",
        "symplify/phpstan-rules": "^9.0",
        "symplify/phpstan-extensions": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Rector\\Website\\": "src",
            "Rector\\Website\\Blog\\": "packages/blog/src",
            "Rector\\Website\\Demo\\": "packages/demo/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rector\\Website\\Tests\\": "tests",
            "Rector\\Website\\Demo\\Tests\\": "packages/demo/tests"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "check-cs": "vendor/bin/ecs check --ansi",
        "fix-cs": "vendor/bin/ecs check --fix --ansi",
        "doctrine": "bin/console doctrine:schema:update --dump-sql --force --ansi",
        "phpstan": "php -d memory_limit=1G vendor/bin/phpstan analyse --error-format symplify --ansi",
        "rector": "vendor/bin/rector process --config rector-ci.php --ansi"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.1.*"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
